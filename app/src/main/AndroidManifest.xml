<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.davidbezalellaoli.mobile">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_kota_medan_new"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Main"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".modules.auth.activities.AuthNavigation" android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>
        <activity android:name=".modules.auth.activities.LoginCitizen" android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>
        <activity android:name=".modules.auth.activities.RegisterCitizen" android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>
        <activity android:name=".modules.auth.activities.LoginAdmin" android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".modules.citizen.activities.Main" />
    </application>

</manifest>